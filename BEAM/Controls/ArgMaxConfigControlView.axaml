<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:BEAM.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="controls:ArgMaxConfigControlViewModel"
             x:Class="BEAM.Controls.ArgMaxConfigControlView">
    <DataGrid Margin="20" ItemsSource="{Binding ChannelHsvMap}"
              IsReadOnly="True"
              CanUserReorderColumns="True"
              CanUserResizeColumns="True"
              CanUserSortColumns="False"
              GridLinesVisibility="All"
              BorderThickness="1" BorderBrush="Gray">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Channel Number"  Binding="{Binding ChannelHsvMap.}"/>
            <DataGridTextColumn Header="Last Name" Binding="{Binding LastName}" />
        </DataGrid.Columns>
    </DataGrid>
</UserControl>